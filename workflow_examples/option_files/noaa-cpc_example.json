{
    "TAGS":{
        "source"   : "NOAA",
        "product"  : "CPC_global_precip",
        "prod_name": "{source}-{product}"
    },
    "DATASETS":{
        "destination" : {
            "type" : "local",
            "path" : "output/{source}/{product}/%Y",
            "filename" : "{prod_name}_sample_%Y%m%d.tif"
        }
    },
    "DOOR_DOWNLOADER" : {
        "source"    : "{source}",
        "product"   : "{product}",
        "destination" : "{DATASETS.destination}",
        "bounds" : [-180, -90, 180, 90],
        "options" : {
            "ts_per_year" : 36
        }
    }
}