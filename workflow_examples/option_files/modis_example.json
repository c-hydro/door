{
    "TAGS":{
        "source"   : "MODIS",
        "product"  : "et",
        "prod_name": "{source}-{variable}"
    },
    "DATASETS":{
        "destination" : {
            "type" : "local",
            "path" : "output/{source}/{variable}/%Y",
            "filename" : "{prod_name}_sample_%Y%m%d.tif"
        }
    },
    "DOOR_DOWNLOADER" : {
        "source"    : "{source}",
        "product"   : "{product}",
        "destination" : "{DATASETS.destination}",
        "bounds" : [8, 38.5, 10, 41.5],
        "options" : {
            "variables": ["ET", "ET_QC"]
        }
    }
}